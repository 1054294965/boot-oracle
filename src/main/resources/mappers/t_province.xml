<mapper namespace="com.jeesite.modules.msg.dao.MsgInnerDao">
    <resultMap id="provinceResultMap" type="com.example.entity.Province">

        <result property="provName" column="PROV_NAME"/>
        <result property="bb" column="BB"/>
        <result property="by1" column="BY1"/>
        <result property="by2" column="BY2"/>
        <result property="drsj" column="DRSJ"/>
